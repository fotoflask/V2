<!DOCTYPE html>
<html>
    <head>
     <title>Fotoflask</title>
     <link rel="stylesheet" href="/css/homepage_s.css">
     <link rel="stylesheet" href="/css/navbar.css">
     <link rel="stylesheet" href="/css/search1.css">
     <link rel="stylesheet" href="/css/loadinganimation1.css">
     <link rel="stylesheet" href="/css/leftsidebar.css">
		<meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        <link href="https://fonts.googleapis.com/css2?family=Livvic:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,900&display=swap" rel="stylesheet">
    </head>
    <body>

        
        <%- include("layout/loadinganimation2") %> 
        <%- include("layout/navbar",{user_logged}) %>
        
        <%- include("layout/bottomnavbar") %>

        <div class="ccontainer">

            <!--Left Side Bar-->

            <%- include("layout/leftsidebar") %>

            <!--Main Content-->

            
            <div class="maincontent">


           
                <div class="body1">
                    <div class="container1">
                        <header class="header1">
                          <input type="text" id="filter" style="background-color: white;color: black;" placeholder="Search">
                        </header>
                  
                        <ul id="result" class="user-list style-1">
                            <% doc2.forEach(doc => { %>
                                <li>
                                    <img src="/images/profilephotos/<%=doc.dprofilepic%>" alt="<%=doc.dusername%>" onerror="this.onerror=null; this.src='/images/OtherAssets/pngwing.com (2).png'; style='object-fit:scale-down;'">
                                    <div class="user-info">
                                        <h4><%=doc.dprofilename%></h4>
                                        <p><%=doc.dusername%></p>
                                    </div>
                                    <div>
                                        
                                        <%- include("layout/follow",{doc}) %>
                                    </div>
                                </li>
                             <% }) %>
                          
                        </ul>
                      </div>
                </div>
            </div>

            <!--Right Side Bar-->
            <div class="rightsidebar style-1">   

                <div class="advertisements">
                    <div class="sidebartitle">
                        <h4>Sponsored</h4>
                        
                    </div>
                    <div class="add-image">
                        <img src="/images/otherassets/Fotoflask.gif" class="sidebarads">
                        
                    </div>
                </div>

                <div class="sidebartitle">
                    <h4>Events</h4>
                    <a href="#">See All</a>
                </div>
                <div class="events-container style-1">
                    <div class="events">
                        <div class="leftevent">
                            <h3 class="eventdate">19</h3>
                            <span><b>August</b></span>

                        </div>
                        <div class="rightevent">
                            <h4>National Photography Day</h4>
                            <a href="https://www.daysoftheyear.com/days/photography-day/">More Info</a>

                        </div>

                    </div>

                    <div class="events">
                        <div class="leftevent">
                            <h3 class="eventdate">05</h3>
                            <span><b>October</b></span>
                        </div>
                        <div class="rightevent">
                            <h4>Photography Contest 2023</h4>
                            <a href="#">More Info</a>
                        </div>

                    </div>
                    <div class="events">
                        <div class="leftevent">
                            <h3 class="eventdate">29</h3>
                            <span><b>April</b></span>

                        </div>
                        <div class="rightevent">
                            <h3>International Photography Day</h4>
                            <a href="https://www.daysoftheyear.com/days/photography-day/">More Info</a>

                        </div>

                    </div>
                </div>

                


                <div class="sidebartitle">
                    <h4>Chat</h4>
                    <a href="#">Hide Chat</a>
                </div>

                <div class="online-profiles style-1">
                    <div class="onlinelist">

                        <div class="online">
                            <img src="/images/ProfilePhotos/sravan.png">
                        </div>
                        <p>Sravan</p>
                    </div>

                    <div class="onlinelist">

                        <div class="online">
                            <img src="/images/ProfilePhotos/poojyanth.png">
                        </div>
                        <p>Poojyanth</p>
                    </div>

                    <div class="onlinelist">

                        <div class="online">
                            <img src="/images/ProfilePhotos/abhi.jpg">
                        </div>
                        <p>Abhiram</p>
                    </div>
                    <div class="onlinelist">

                        <div class="online">
                            <img src="/images/ProfilePhotos/pavan.png">
                        </div>
                        <p>Sai Pavan</p>
                    </div>
                </div>

            </div>


        </div>

        <script src="/js/script1.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        <script src="https://kit.fontawesome.com/5e315e4af1.js" crossorigin="anonymous"></script>
        <script src="https://code.jquery.com/jquery-3.6.3.js" integrity="sha256-nQLuAZGRRcILA+6dMBOvcRh5Pe310sBpanc6+QBmyVM=" crossorigin="anonymous"></script>
        <script src="/js/homepage.js"></script>
        <script>
            $.holdReady(true);
        setTimeout(function () {
  
            // Setting a variable until
            // document.ready is called
            const myVar = "GFG";
            $.holdReady(false);
        }, 2000);
         $(document).ready(()=> {
            //Preloader
            preloaderFadeOutTime = 500;
            function hidePreloader() {
            var preloader = $('.cube-wrapper');
            preloader.fadeOut(preloaderFadeOutTime);
            }
            hidePreloader();
            });
          </script>

    </body>
</html>